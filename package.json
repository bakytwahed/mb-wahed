{
  "name": "mb-graphql",
  "version": "0.1.3",
  "description": "mountebank test doubles for GraphQL",
  "author": "bashj79",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bashj79/mb-graphql.git"
  },
  "bin": {
    "mb-graphql": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "build": "npm run lint && npm run compile && npm test",
    "compile": "rimraf dist && babel src --out-dir dist --ignore '**/*.test.js'"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "mountebank",
    "graphql",
    "test",
    "stub",
    "mock",
    "double"
  ],
  "dependencies": {
    "@graphql-tools/delegate": "^7.0.10",
    "@graphql-tools/load": "^6.2.7",
    "@graphql-tools/url-loader": "^6.8.1",
    "@graphql-tools/wrap": "^7.0.5",
    "apollo-server": "^2.21.0",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.4",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "babel-jest": "^26.6.3",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  }
}
